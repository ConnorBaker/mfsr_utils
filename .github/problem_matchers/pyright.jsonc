{
  "problemMatcher": [
    {
      "owner": "pyright",
      "pattern": [
        {
          // GitHub understands case-insentive severity levels
          "regexp": "^  ([/\\w.]+):(.+):(.+) - (error|warning|info): (.+) \\((.+)\\)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}